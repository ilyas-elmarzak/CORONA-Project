{
  "name": "dist",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "node-sass sass/style.scss css/style.css --watch",
    "compile": "node-sass sass/style.scss css/style.comp.css",
    "concat": "concat -o css/style.css css/nav.css css/main.css css/section.css",
    "prefix": "postcss css/style.concat.css --use=autoprefixer --map=false --output=css/style.prefix.css",
    "compress": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "comp-js": "uglifyjs js/script.js -o js/main.js",
    "build-css": "npm-run-all watch compile concat prefix compress"
  },
  "author": "ILYAS ELMARZAK",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
